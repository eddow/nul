<div align="center">{+new Link().toFile("index.html").withText("Class Index")+}
| {+new Link().toFile("files.html").withText("File Index")+}</div>
<hr />

<link type="text/css" rel="stylesheet" href="../trunk/3rd/jquery/theme/treeTable.css"></link>
<script type="text/javascript" src="../trunk/3rd/jquery/jquery.js"></script>
<script type="text/javascript" src="../trunk/3rd/jquery/treeTable.js"></script>

<h2>Classes</h2>
<table class="classList">
	<for each="thisClass" in="data">
	{!
		var ar = thisClass.alias.split(".");
		var nid = ar.join("_");
		ar.pop();
		var ncls = ar.join("_");
		output += '<tr id="'+nid+'" ';
		if(ncls) output += 'class="child-of-'+ncls+'"';
		output += '><td>';
		if (thisClass.alias == "_global_") {
			output += "<i>"+new Link().toClass(thisClass.alias)+"</i>";
		}
		else {
			output += new Link().toClass(thisClass.alias);
		}
	!}</td></tr>
	</for>
</table>

<script type="text/javascript">
	$('table.classList').treeTable({ indent: 16, initialState: 'collapsed' });
</script>


<hr />