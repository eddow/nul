set = <library

/* 'set.tuple[A, B, C]' takes tuples for whose items are respectively from set A, B and C
 set.tuple[Q, str] (5, "ok") 
*/
tuple
={	{} => { {} }
[]	(T,.. Ts) => ( T _,.. set.tuple[Ts] )
}

sub
={ {} => {}
[] (a,.. r) => (a,.. set.sub r)
[] (_,.. r) => set.sub r 
}

sigma
={ \/x {x} => {}
[] (a,b,.. o) => ((a=>b),.. set.sigma[b,.. o] )
}

id
={ x; x != (p => _!=p) }

/>